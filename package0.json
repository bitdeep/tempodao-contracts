{
  "name": "MaticSwap",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "matic:testnet:timelock": "truffle migrate --f 2 --to 2 --network matic_testnet",
    "matic:mainnet:timelock": "truffle migrate --f 2 --to 2 --network matic_mainnet",
    "matic:testnet:farm": "truffle migrate --f 3 --to 3 --network matic_testnet",
    "matic:mainnet:farm": "truffle migrate --f 3 --to 3 --network matic_mainnet",
    "matic:testnet:factory": "truffle migrate --f 4 --to 4 --network matic_testnet",
    "matic:mainnet:factory": "truffle migrate --f 4 --to 4 --network matic_mainnet",
    "matic:testnet:weth": "truffle migrate --f 5 --to 5 --network matic_testnet ",
    "matic:mainnet:weth": "truffle migrate --f 5 --to 5 --network matic_mainnet ",
    "matic:testnet:router": "truffle migrate --f 6 --to 6 --network matic_testnet ",
    "matic:mainnet:router": "truffle migrate --f 6 --to 6 --network matic_mainnet ",
    "dev:farm": "truffle migrate --f 3 --to 3 --network dev --reset",
    "dev:factory": "truffle migrate --f 4 --to 4 --network dev --reset",
    "dev:weth": "truffle migrate --f 5 --to 5 --network dev --reset",
    "dev:router": "truffle migrate --f 6 --to 6 --network dev --reset",
    "migrate": "truffle migrate --network dev --reset",
    "compile": "truffle compile --network dev"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.2.0",
    "@openzeppelin/test-helpers": "^0.5.6",
    "@truffle/hdwallet-provider": "^1.4.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.14",
    "solidity-coverage": "^0.7.13",
    "truffle": "^5.1.58",
    "truffle-flattener": "^1.4.4",
    "truffle-hdwallet-provider": "^1.0.17",
    "truffle-plugin-verify": "^0.5.4",
    "truffle-privatekey-provider": "^1.5.0",
    "web3": "^1.3.0",
    "web3-provider-engine": "^16.0.1"
  }
}
